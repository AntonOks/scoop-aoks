{
    "version": "1.4.4",
    "description": "Desktop app to connect to a running Boundary server",
    "homepage": "https://www.boundaryproject.io",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://releases.hashicorp.com/boundary-desktop/1.4.4/boundary-desktop_1.4.4_windows_amd64.zip",
            "hash": "870ba98de2d4ccf1144c887b48e4d359b19f64fedb0cf716fdefd706f161bd52"
        }
    },
    "extract_dir": "Boundary",
    "bin": [
