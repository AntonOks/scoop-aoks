        "file": "ac.exe",
        "args": [
            "/VERYSILENT",
            "/NOCANCEL",
            "/NORESTART",
            "/DIR=\"$dir\"",
            "/LOG=\"$dir\\ac.log\"",
            "/SUPPRESSMSGBOXES",
            "/CLOSEAPPLICATIONS",
