{
    "homepage": "https://www.petges.lu/",
    "version": "11.30b",
    "description": "Easily change file and folder properties with a right-click in Explorer.",
    "license": "Proprietary",
    "url": "https://www.petges.lu/pubfiles/ac-11_30b.exe#/ac.exe",
