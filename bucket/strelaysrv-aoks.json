{
    "version": "2.0.9",
    "description": "The Syncthing relay server",
    "homepage": "https://syncthing.net/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/syncthing/relaysrv/releases/download/v2.0.9/strelaysrv-windows-amd64-v2.0.9.zip",
            "hash": "abe93b6918f893a3cee0bd607bf8d159ec75e370320e7e71fbb6baf3a7e753c9",
            "extract_dir": "strelaysrv-windows-amd64-v2.0.9"
        },
        "32bit": {
            "url": "https://github.com/syncthing/relaysrv/releases/download/v2.0.9/strelaysrv-windows-386-v2.0.9.zip",
            "hash": "8d36b8086e65440a66fb4f08bd842f90f1166ed5b4f5f33219bb159b37d526fc",
            "extract_dir": "strelaysrv-windows-386-v2.0.9"
        }
    },
    "bin": [
        [
            "strelaysrv.exe",
            "strelaysrv",
            "-keys \"$dir\""
        ]
    ],
    "persist": [
        "cert.pem",
        "key.pem"
    ],
    "pre_install": [
        "foreach ($name in @('cert.pem', 'key.pem')) {",
        "    if (!(Test-Path \"$persist_dir\\$name\")) {",
        "        New-Item -Force -Path \"$dir\" -Name $name | Out-Null",
        "    }",
        "}"
    ],
    "checkver": {
        "github": "https://github.com/syncthing/relaysrv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/syncthing/relaysrv/releases/download/v$version/strelaysrv-windows-amd64-v$version.zip",
                "extract_dir": "strelaysrv-windows-amd64-v$version"
            },
            "32bit": {
                "url": "https://github.com/syncthing/relaysrv/releases/download/v$version/strelaysrv-windows-386-v$version.zip",
                "extract_dir": "strelaysrv-windows-386-v$version"
            }
        }
    }
}
