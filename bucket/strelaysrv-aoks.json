{
    "version": "2.0.5",
    "description": "The Syncthing relay server",
    "homepage": "https://syncthing.net/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/syncthing/relaysrv/releases/download/v2.0.5/strelaysrv-windows-amd64-v2.0.5.zip",
            "hash": "efceb37d527c0a913597dfcb4366c7c224e2b5361cecf9253c29c7e873f2d54c",
            "extract_dir": "strelaysrv-windows-amd64-v2.0.5"
        },
        "32bit": {
            "url": "https://github.com/syncthing/relaysrv/releases/download/v2.0.5/strelaysrv-windows-386-v2.0.5.zip",
            "hash": "aaf2fe21e85d73a1dfe12f0351863caaa8e7c8450ac30dd24e5a55520531356d",
            "extract_dir": "strelaysrv-windows-386-v2.0.5"
        }
    },
    "bin": [
        [
            "strelaysrv.exe",
            "strelaysrv",
            "-keys \"$dir\""
        ]
    ],
    "persist": [
        "cert.pem",
        "key.pem"
    ],
    "pre_install": [
        "foreach ($name in @('cert.pem', 'key.pem')) {",
        "    if (!(Test-Path \"$persist_dir\\$name\")) {",
        "        New-Item -Force -Path \"$dir\" -Name $name | Out-Null",
        "    }",
        "}"
    ],
    "checkver": {
        "github": "https://github.com/syncthing/relaysrv"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/syncthing/relaysrv/releases/download/v$version/strelaysrv-windows-amd64-v$version.zip",
                "extract_dir": "strelaysrv-windows-amd64-v$version"
            },
            "32bit": {
                "url": "https://github.com/syncthing/relaysrv/releases/download/v$version/strelaysrv-windows-386-v$version.zip",
                "extract_dir": "strelaysrv-windows-386-v$version"
            }
        }
    }
}
